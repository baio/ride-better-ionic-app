ion-view(ng-attr-title="{{state.spot.title}}")
    ion-content.has-header      
        ion-slide-box(ng-if="prices.length !== 0" does-continue="true")
            ion-slide.box(ng-repeat="price in prices")
              .card
                .item.item-divider
                  {{price.title}}
                .item.item-text-wrap
                  img(width="100%" ng-src="{{price.src}}")
                .item.item-divider
                  {{price.created |  amCalendar}}



